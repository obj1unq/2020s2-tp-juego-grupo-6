import wollok.game.*
import player.*
import config.*

program UNQgame {
	game.title("AmUNQ Us")
	
	//VISUALES
	game.addVisual(player)
	
	//CONFIGURACIONES
	game.height(10)
	game.width(25)
	
	//Fondo
	game.boardGround("fondo1.jpg")
	
	//Musica
	game.sound("amongUsTrap.wav")
	
	config.configurarTeclas()
	
	//DEBUG
	game.showAttributes(player)
	
	//START
	game.start()
}